<?xml version="1.0" encoding="utf-8" ?>
<log4net>
	<appender name="A1" type="log4net.Appender.ConsoleAppender">

		<layout type="log4net.Layout.PatternLayout">
			<conversionPattern value="%-4timestamp %-5level - %message%newline" />
		</layout>
	</appender>

	<root>
		<level value="DEBUG" />
		<appender-ref ref="A1" />
	</root>

	<logger name="Stact">
		<level value="WARN"/>
	</logger>
</log4net>